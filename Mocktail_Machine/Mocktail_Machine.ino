enum Outputs
{
  Kokosmaelk,
  Limejuice,
  Danskvand,
  Grenadine,
  Appelsinjuice,
  BlaaCuracao,
  LemonSoda,
  MangoSirup
};

class Mocktail {
  public:
    String Name;
    static const int MAX_SIZE = 10;
    Outputs Ingredients[MAX_SIZE];
    int Ratios[MAX_SIZE];
    int Size;

    Mocktail(String name, const Outputs ingredients[], const int ratios[], int size) 
      : Name(name), Size(size) {

      for (int i = 0; i < size; i++) {
        Ingredients[i] = ingredients[i];
        Ratios[i] = ratios[i];
      }
    }
};

