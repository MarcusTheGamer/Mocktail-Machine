const int cupSize = 250;

void displayMocktail(const uint8_t* const image [73][29], int offsetX = 0)
{
  for (uint8_t y = 0; y < 73; y++)
  {
    for (uint8_t x = 0; x < 29; x++)
    {
      if (pgm_read_word_near(&image[y][x]) != 0xFFFF) mylcd.Fill_Rect(x*2 + 91 + offsetX,y*2 + 47,2,2,pgm_read_word_near(&image[y][x]));
    }
  }
}

void displayBackground(int top [3], int bottom [3])
{
  int rgb [3];
  for (int y = 0; y < 12; y++)
  {
    for (int i = 0; i < 3; i++)
    {
      rgb[i] = round((bottom[i] - top[i]) / 240. * (y * 20.)) + top[i];
    }
    
    uint16_t RGB = ((rgb[0] & 0xF8) << 8) | ((rgb[1] & 0xFC) << 3) | (rgb[2] >> 3);
    mylcd.Fill_Rect(0,y * 20,240,20,RGB);
  };
}

void setup() {
  pinMode(Module1, OUTPUT);
  pinMode(Module2, OUTPUT);
  pinMode(Module3, OUTPUT);
  pinMode(Module4, OUTPUT);
  pinMode(Module5, OUTPUT);
  pinMode(Module6, OUTPUT);
  pinMode(Module7, OUTPUT);
  pinMode(Module8, OUTPUT);

  pinMode(NextBtn, INPUT);
  pinMode(SelectBtn, INPUT);
  pinMode(PrevBtn, INPUT);

  mylcd.Init_LCD();
  Serial.begin(9600);

  displayBackground(HomeTop,HomeBottom);
}


int SelectedIndex = 0;
int CurrentModule = 0;
bool IsPouring = false;

void pour(int index)
{
  IsPouring = true;
  Mocktail mocktail = mocktails[index];

  for (int i = 0; i < sizeof(mocktail.Ingredients) / sizeof(mocktail.Ingredients[0]); i++)
  {
    int index = 0;
    for (int j = 0; j < 8; j++)
    {
      if (mocktail.Ingredients[i] == Outputs(j)) index = j;
    }

    int amount = cupSize / 100 * mocktail.Ratios[i];
    int HoldUpWaitAMinute = amount * 10;

    digitalWrite(CurrentModule, LOW);
    digitalWrite(Modules[index], HIGH);
    CurrentModule = Modules[index];
    delay(HoldUpWaitAMinute);

  };
  digitalWrite(CurrentModule, LOW);
  IsPouring = false;
}

